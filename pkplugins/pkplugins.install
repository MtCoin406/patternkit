/**
 * Add Schema for cache data.
 */
function pkplugins_schema() {
  $schema = array();

  // Add the redhat_pkplugins caching table.
  $schema['cache_pkplugins'] = drupal_get_schema_unprocessed('system', 'cache');

  return $schema;
}
